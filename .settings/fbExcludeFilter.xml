<!-- Do not edit this file, as this is used for each project -->
<FindBugsFilter>

  <!-- Ignore serialization issues-->
  <Match>
    <Bug code="Se, TEST" />
  </Match>

  <!-- Ignore public constants and likes -->
  <Match>
    <Bug
      pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD, UUF_UNUSED_PUBLIC_OR_PROTECTED_FIELD, UWF_UNWRITTEN_PUBLIC_OR_PROTECTED_FIELD" />
  </Match>

  <!-- Ignore internationalization and experimental issues-->
  <Match>
    <Bug category="NOISE,I18N" />
  </Match>

  <!-- Ignore generated classes -->
  <Match>
    <Or>
      <Package name="~xoc\*" />
      <Package name="~testflow\*" />
    </Or>
  </Match>

     <!-- ignore low prio bugs for the specified patterns. -->
     <Match>
       <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR,
        SIC_INNER_SHOULD_BE_STATIC_ANON" />
       <Priority value="3" />
     </Match>
     <Match>
       <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD,
        UUF_UNUSED_PUBLIC_OR_PROTECTED_FIELD,
        UWF_UNWRITTEN_PUBLIC_OR_PROTECTED_FIELD" />
     </Match>

     <!--
        Xtext specific issues.
      -->
      <Match>
        <And>
          <Bug pattern="DLS_DEAD_LOCAL_STORE" />
          <Local name="t" type="org.eclipse.xtext.common.types.JvmTypeReference" />
       </And>
     </Match>


</FindBugsFilter>
